      - text: Lurelin Village
      - control:
          kind: reset_colour
      - text: ".\nThey say the fish and fruit there are\nreally fantastic, so I thought I'd go there\nand add them to my diet!"
  MSG_Firo_00_1:
    attributes: Npc_Road_009
    contents:
      - control:
          kind: sound
          unknown:
            - 5
            - 0
      - text: "I'm on my way to "
      - control:
