  TalkHatu_01:
    attributes: Npc_RitoHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 2
      - text: "Welcome to my "
      - control:
          kind: set_colour
          colour: blue
      - text: stable
      - control:
          kind: reset_colour
      - text: ".\nWe deal in all things horse related.\nWe also double as an inn.\n"
      - control:
          kind: raw
          four:
            three:
              field_1: 0
      - text: Huh...
      - control:
          kind: pause
          length: short
      - text: " I haven't seen your face before."
      - control:
          kind: pause
          length: longer
      - text: "\nDo you know about the "
      - control:
          kind: set_colour
          colour: blue
      - text: stable
      - control:
          kind: reset_colour
      - text: " system?"
      - control:
