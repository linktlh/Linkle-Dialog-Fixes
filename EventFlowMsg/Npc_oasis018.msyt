          kind: sound
          unknown:
            - 0
            - 25
      - text: "My mom often said that dishes with\nmushrooms were good for nursing\nserious headaches.\nSo..."
      - control:
          kind: pause
          length: short
      - text: "when I'm feeling tired, I always make\n"
      - control:
          kind: set_colour
          colour: blue
      - text: fragrant mushroom sauté
      - control:
          kind: reset_colour
      - text: ".\nI love that curry flavor!\nIt's a simple dish that you can make if\nyou have some "
      - control:
          kind: set_colour
          colour: blue
      - text: Goron spice
      - control:
          kind: reset_colour
      - text: " and\n"
      - control:
