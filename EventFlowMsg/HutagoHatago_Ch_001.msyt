    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "My record is two minutes flat, so you\nneed to bring back a wild horse within\nthat time.\n"
      - control:
          kind: sound
          unknown:
            - 7
            - 0
