  Appear16:
    attributes: Npc_King_Parasail002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: "Here, on this isolated plateau, "
      - control:
          kind: pause
          length: short
      - text: "we are\nsurrounded on all sides by steep cliffs,\nwith no way down."
  Appear17:
    attributes: Npc_King_Parasail002
    contents:
