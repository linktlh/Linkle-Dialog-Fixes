            - 8
            - 14
      - text: HEY!
      - control:
          kind: pause
          length: short
      - text: " You've got some gall waking up a\nsleeping stranger! Show a little respect!"
  talk_sleep01:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "I'M SLEEPY! GO AWAY!"
  talk_sleep02:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 21
      - text: "HEY! You've got some nerve waking\nup a sleeping stranger. "
      - control:
          kind: pause
          length: longer
      - text: And...
      - control:
          kind: pause
          length: longer
      - text: "\nWHERE ARE YOUR CLOTHES?!\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "GET AWAY FROM ME, YOU CREEP!"
      - control:
          kind: sound
          unknown:
            - 8
            - 14
  talk_sleep03:
    attributes: Npc_RiverSideHatago002
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 14
      - text: "Here's some friendly advice—don't wake\nup strangers when you're not wearing\nclothes,"
      - control:
          kind: pause
          length: long
      - text: " you little creep."
