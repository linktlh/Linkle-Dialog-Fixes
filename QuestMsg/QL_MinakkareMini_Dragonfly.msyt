            field_1: 2
            field_2: 0
    - text: cold darner
    - control:
        zero:
          type3:
            field_1: 2
            field_2: 65535
    - text: ",\nand one "
    - control:
        zero:
          type3:
            field_1: 2
            field_2: 0
    - text: electric darner
    - control:
