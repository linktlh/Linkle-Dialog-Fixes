    attributes: Obj_DiaryMusician_A_01
    contents:
      - text: "\"The ancient song of "
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo Tower
      - control:
          kind: reset_colour
      - text: ".\"\n\n\nI've heard word there is an ancient\nsong about a tall structure called\n"
      - control:
          kind: set_colour
          colour: blue
      - text: Gerudo Tower
      - control:
          kind: reset_colour
      - text: ".\nI don't know where this "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo Tower "
      - control:
          kind: reset_colour
      - text: "is,\nbut mysterious towers have popped up\nall around Hyrule recently.\nThis "
      - control:
          kind: set_colour
          colour: blue
      - text: "Gerudo Tower "
      - control:
          kind: reset_colour
      - text: "may be the tower that\nappeared in the Gerudo region."
      - control:
          kind: choice
          choice_labels:
            - 3
            - 4
            - 5
          selected_index: 0
          cancel_index: 2
